# By Yule

# 调整过程的步数(一步≈0.04秒)
# 决定 开始调整后到调整完成的时长
step_num=50

# 自定义触发值(0为默认)
# 决定 触发调整的时机
custom_thr_bri=0

# 自定义最大亮度(0为默认)
# 决定 要调整到的亮度
custom_max_bri=0

# 开机后的等待时间(秒)
# 决定 开机后等待的时间
boost_wait_time=30

# 每次刷新的间隔时间(秒)
# 决定 每次检查亮度之间的时间间隔
flash_wait_time=3

# 亮度提升模式 (填1为直接提升，填2为平滑提升)
# 低性能的设备建议使用模式 1
bri_update_mode=2

# 休眠时间(例如你要晚上0点到凌晨7点不运行，那你就填0和7，不开启休眠都填25) (不允许小数点，只能填大于等于0的自然数)
sleep_start=25
sleep_stop=25